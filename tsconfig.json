{
  "compilerOptions": {
    "allowSyntheticDefaultImports": false,
    "declaration": false,
    "forceConsistentCasingInFileNames": false,
    "lib": [
      "dom",
      "es2015"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "preserveConstEnums": true,
    "outDir": "dist",
    "sourceMap": true,
    "strict": true,
    "target": "es5"
  },
  "include": [
    "src",
    "test"
  ]
}